{
  "gitbook": "2.x.x",
  "structure": {
      "readme": "README.adoc"
  },
  "plugins": [
      "ungrey",
      "splitter",
      "expandable-chapters",
      "edit-link",
      "addcssjs",
      "ga"
  ],
  "pluginsConfig": {
          "edit-link": {
             "base": "https://github.com/wildfly-swarm/wildfly-swarm-users-guide/tree/master/",
             "label": "Edit This Page"
          },
          "addcssjs": {
              "css": ["/styles/asciidoc.css"]
          },
          "ga": {
              "token": "UA-83419890-1"
          }
   },
  "variables": {
      "javadoc_url_preffix" : "http://wildfly-swarm.github.io/wildfly-swarm-javadocs/",
      "versions": {
          "wildfly": "10.1.0.Final",
          "swarm": "2017.5.0",
          "configuration": "1.0.2.Final",
          "keycloak": "2.5.0.Final"
      }
  }

}
